<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Korisnik" repository-class="AppBundle\Entity\KorisnikRepository" table="korisnik">
    <unique-constraints>
      <unique-constraint name="korisnicko_ime_UNIQUE" columns="korisnicko_ime"/>
      <unique-constraint name="lozinka_UNIQUE" columns="lozinka"/>
    </unique-constraints>
    <id name="idKorisnik" type="integer" column="id_korisnik">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="korisnickoIme" type="string" column="korisnicko_ime" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lozinka" type="string" column="lozinka" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
